<!DOCTYPE html>
<html>
    <head>
        <% include ../partial/common/head %>
    </head>
    <body>
        <div class='header-wrap'>
            <% include ../partial/common/header %>
        </div>
        <div class='content'>
            <ul class='review-list'>
                <% reviews.forEach(function(review) { %>
                <li>
                    <span class='thumb'><img src='/static/img/no_image_available.png'></span>
                    <span class='title'><a href='/view/<%=review.pageId%>'><%= review.title %></a></span>
                    <span class='tag'><%= review.tag %></span>
                    <span class='nick'></span>
                    <span class='score'><%= review.score %></span>
                    <span class='etc' style='display:none'><%= review.date %><%= review.content %></span>
                </li>
                <% }); %>
            </ul>
        </div>
        <div class='footer-wrap'>
            <% include ../partial/common/footer %>
        </div>
    </body>
    <% include ../partial/common/foot %>
</html>
